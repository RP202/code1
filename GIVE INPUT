//WAP A FCN TO GIVE RADIUS OF CIRCLE
#include <stdio.h>


int main()
{ 
 float radius;
 float area;
 printf("Enter a value for radius :");
 scanf(" %f",&radius);
 float pi=3.14;
 area= radius*radius*pi;
 printf("Area of the radius will be %f",area);
    
    return 0;
}

// Online C compiler to run C program online
#include <stdio.h>
int main()
{ 
 int a,b,c;
 printf("Give a value for a :");
 scanf("%d" ,&a);
  printf("Give a value for b:");
 scanf("%d" ,&b);
  printf("Give a value for c :");
 scanf("%d" , &c);
 int x,y;
 x=a+b+c;
 y=a*b*c;
 printf("Sum of all there numbers are: %d\n",x );
 printf("Muktiplication of three numbers are: %d\n",y);
}
